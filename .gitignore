# .gitignore

# Ignore all files and folders generated by the environment
# backend (Python)
backend/*.pyc
backend/*.pyo
backend/*.pyd

# Ignore all __pycache__ folders in the project
**/__pycache__/
**/*.py[cod]

# Ignore the virtual environment
backend/env/
backend/venv/
backend/.venv/

# Ignore editor configuration files
backend/.vscode/
backend/.idea/

# Ignore test files and generated logs
backend/.pytest_cache/
backend/.mypy_cache/
backend/.coverage
backend/coverage/
backend/coverage.xml
backend/pytest.ini
backend/tox.ini
backend/.env

# frontend (Next.js)
frontend/node_modules/
frontend/.next/
frontend/.env.local
frontend/.env.development.local
frontend/.env.test.local
frontend/.env.production.local
frontend/.vercel/
frontend/.swc/
frontend/.turbo/
frontend/.eslintcache
frontend/coverage/

# Ignore log files
frontend/npm-debug.log*
frontend/yarn-debug.log*
frontend/yarn-error.log*
frontend/pnpm-debug.log*

# Ignore editor configuration files
frontend/.vscode/
frontend/.idea/

# Ignore system files
.DS_Store
Thumbs.db

# Ignore log files at root level
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Ignore root-level dependency installation files
node_modules/
# test

